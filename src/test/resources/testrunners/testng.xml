<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Regression Suite" verbose="3" thread-count="2" parallel="tests">
	<test name="Google Test">
		<parameter name="url" value="https://www.google.com/" />
		<parameter name="browser" value="chrome" />
		<parameter name="headless" value="true" />
		<classes>
			<class name="testngsessions.GoogleTest" />
		</classes>
	</test> <!-- Test -->

	<test name="Open Cart Login Test">
		<parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/login" />
		<parameter name="browser" value="firefox" />
		<parameter name="headless" value="true" />
		<classes>
			<class name="testngsessions.OpenCartLoginTest" />
		</classes>
	</test> <!-- Test -->
	<test name="Orange CRM Test">
		<parameter name="url" value="https://orangehrm.com/en/30-day-free-trial" />
		<parameter name="browser" value="edge" />
		<parameter name="headless" value="true" />
		<classes>
			<class name="testngsessions.OrangeHRMTest"> </class>
			<!-- <methods>
				<include name="urlTest"> </include>
			</methods>-->
		</classes>

	</test> <!-- Test -->
</suite> <!-- Suite -->